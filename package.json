{
  "name": "causadb",
  "version": "1.2.8",
  "description": "Node client for CausaDB, a cloud platform for building and using causal AI.",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.js",
  "scripts": {
    "clean": "rm -rf node_modules && rm -rf coverage && rm -rf dist",
    "build": "tsc",
    "lint": "eslint 'src/**/*.{js,ts}' --fix  --no-error-on-unmatched-pattern",
    "test": "jest --config jest.config.cjs --verbose --coverage --silent -u --passWithNoTests --runInBand",
    "docs": "typedoc --disableSources --out docs --plugin typedoc-plugin-markdown ./src && node ./scripts/convert_docs.js",
    "prepublish": "yarn build"
  },
  "author": "Jordan Hart <jordan@causa.tech>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/causalabs/causadb-node.git"
  },
  "devDependencies": {
    "@eslint/js": "^9.2.0",
    "@stylistic/eslint-plugin": "^2.1.0",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.17",
    "eslint": "^9.2.0",
    "eslint-plugin-jest": "^28.5.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "typedoc": "^0.25.8",
    "typedoc-plugin-markdown": "^3.17.1",
    "typescript": "^5.4.5",
    "typescript-eslint": "^7.9.0"
  },
  "dependencies": {
    "axios": "^1.6.7",
    "csv-parser": "^3.0.0",
    "dotenv": "^16.4.1"
  },
  "files": [
    "dist/**/*"
  ],
  "packageManager": "yarn@1.22.19+sha256.732620bac8b1690d507274f025f3c6cfdc3627a84d9642e38a07452cc00e0f2e",
  "bugs": {
    "url": "https://github.com/causalabs/causadb-node/issues"
  },
  "homepage": "https://github.com/causalabs/causadb-node#readme",
  "directories": {
    "test": "tests"
  }
}
